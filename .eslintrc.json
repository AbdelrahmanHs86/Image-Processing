{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        // "airbnb-base",
        "eslint:recommended",
        "prettier"
        // "plugin:node/recommended"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": 2,
        "quotes": ["error", "single"],
        "no-console": 1,
        "no-unused-vars":1,
        "prefer-const": "error"
    }
}
